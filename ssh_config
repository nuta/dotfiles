Host desktop
  HostName seiya-desktop.local
  Port 61386
  ForwardX11Trusted yes

Host gh
  HostName github.com
  User git

Host local
  HostName localhost
  Port 10022
  ForwardX11 yes

Host rpi
  HostName seiya-raspi.local
  Port 50022

Host *
  IdentityFile ~/.ssh/id_ed25519
  AddKeysToAgent yes
  ForwardAgent yes
